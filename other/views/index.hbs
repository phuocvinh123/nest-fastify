  <script>
    function popper (e, show = true,popper = '.popper') {
      const tooltip = e.parentElement.querySelector(popper);
      window.FloatingUIDOM.computePosition(e, tooltip, {
        placement: 'right',
      }).then(({x, y}) => {
        Object.assign(tooltip.style, {
          left: show ? `${x}px` : '-1000px',
          top: show ? `${y}px` : '-1000px',
          opacity: show ? 100 : 0,
        });
      });
    }
  </script>

